{
  extends: [
    "config:base",
    "helpers:pinGitHubActionDigests",
    "github>aquaproj/aqua-renovate-config#1.10.0",
    "github>aquaproj/aqua-renovate-config:installer-script#1.10.0(README\\.md)",
  ],
  automerge: true,
  major: {
    automerge: false,
  },
  regexManagers: [
    {
      fileMatch: ["README.md"],
      depNameTemplate: "aquaproj/aqua-installer",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "aqua-installer@(?<currentValue>.*)",
      ],
    },
    {
      fileMatch: ["README.md"],
      depNameTemplate: "aquaproj/aqua",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "aqua_version: (?<currentValue>.*?)\\n",
        "bash -s -- -v (?<currentValue>.*) -i",
      ],
    },
  ],
}
