{
  extends: [
    "github>suzuki-shunsuke/renovate-config#3.0.0",
    "github>suzuki-shunsuke/renovate-config:nolimit#3.0.0",
    "github>aquaproj/aqua-renovate-config#2.6.0",
    "github>aquaproj/aqua-renovate-config:installer-script#2.7.0(README\\.md)",
  ],
  customManagers: [
    {
      customType: "regex",
      fileMatch: ["README.md"],
      depNameTemplate: "aquaproj/aqua-installer",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "aqua-installer@(?<currentValue>.*)",
      ],
    },
    {
      customType: "regex",
      fileMatch: ["README.md"],
      depNameTemplate: "aquaproj/aqua",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "aqua_version: (?<currentValue>.*?)\\n",
        "bash -s -- -v (?<currentValue>.*) -i",
      ],
    },
  ],
}
